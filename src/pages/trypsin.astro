---
import '../styles/global.css';
import PageLayout from '../layouts/PageLayout.astro';

import Input from '../components/react/Input';

const pageTitle = 'Trypsin digest';
---

<PageLayout pageTitle={pageTitle}>
  <section class="reactArea">
    <Input
      client:load
      ariaLabelContent={'DNA input form for trypsin digest'}
      placeholderText={'Enter protein sequence here...'}
    />
  </section>
</PageLayout>
